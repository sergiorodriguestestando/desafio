{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "id",
        "title",
        "price",
        "stock",
        "rating",
        "thumbnail",
        "description",
        "brand",
        "category"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                101
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Perfume Oil"
            ]
        },
        "price": {
            "type": "integer",
            "default": 0,
            "title": "The price Schema",
            "examples": [
                13
            ]
        },
        "stock": {
            "type": "integer",
            "default": 0,
            "title": "The stock Schema",
            "examples": [
                65
            ]
        },
        "rating": {
            "type": "number",
            "default": 0.0,
            "title": "The rating Schema",
            "examples": [
                4.26
            ]
        },
        "thumbnail": {
            "type": "string",
            "default": "",
            "title": "The thumbnail Schema",
            "examples": [
                "https://i.dummyjson.com/data/products/11/thumnail.jpg"
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "Mega Discount, Impression of A..."
            ]
        },
        "brand": {
            "type": "string",
            "default": "",
            "title": "The brand Schema",
            "examples": [
                "Impression of Acqua Di Gio"
            ]
        },
        "category": {
            "type": "string",
            "default": "",
            "title": "The category Schema",
            "examples": [
                "fragrances"
            ]
        }
    },
    "examples": [{
        "id": 101,
        "title": "Perfume Oil",
        "price": 13,
        "stock": 65,
        "rating": 4.26,
        "thumbnail": "https://i.dummyjson.com/data/products/11/thumnail.jpg",
        "description": "Mega Discount, Impression of A...",
        "brand": "Impression of Acqua Di Gio",
        "category": "fragrances"
    }]
}